@ ECHO off

IF EXIST "./delete_node_modules" (
	CALL :enterFolder
	CALL :getLatest
	CALL :startApp
) else (
    CALL :getProjectFromGit
	CALL :enterFolder
	CALL :startApp
)

:getProjectFromGit
git clone https://github.com/bhaireshm/delete_node_modules.git
CLS
EXIT /B 0

:getLatest
ECHO Getting latest data...
git pull origin master
CLS
EXIT /B 0

:startApp
CALL start.bat
EXIT 0

:enterFolder
cd delete_node_modules
EXIT /B 0
